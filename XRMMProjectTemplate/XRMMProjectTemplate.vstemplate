<?xml version="1.0" encoding="utf-8"?>
<VSTemplate Version="3.0.0" Type="Project" xmlns="http://schemas.microsoft.com/developer/vstemplate/2005" xmlns:sdk="http://schemas.microsoft.com/developer/vstemplate-sdkextension/2010">
  <TemplateData>
    <TemplateID>7f44d369-5d06-4202-a4dc-4285f6fa6358</TemplateID>
    <Name>LINE Rich Menu Maker</Name>
    <Description>A rich menu creation tool for LINE chat bot, which designs rich menus using XAML.</Description>
    <ProjectType>CSharp</ProjectType>
    <LanguageTag>csharp</LanguageTag>
    <SortOrder>1000</SortOrder>
    <CreateNewFolder>true</CreateNewFolder>
    <DefaultName>XamlRichMenuMaker</DefaultName>
    <ProvideDefaultName>true</ProvideDefaultName>
    <LocationField>Enabled</LocationField>
    <EnableLocationBrowseButton>true</EnableLocationBrowseButton>
    <CreateInPlace>true</CreateInPlace>
    <Icon>XRMMProjectTemplate.ico</Icon>
    
  </TemplateData>
    <TemplateContent>
      <Project TargetFileName="XamlRichMenuMaker.csproj" File="XamlRichMenuMaker.csproj" ReplaceParameters="true">
        <Folder Name="Resources" TargetFolderName="Resources">
          <ProjectItem ReplaceParameters="false" TargetFileName="cat.png">cat.png</ProjectItem>
          <ProjectItem ReplaceParameters="false" TargetFileName="web.png">web.png</ProjectItem>
        </Folder>
        <Folder Name="RichMenuDefs" TargetFolderName="RichMenuDefs">
          <ProjectItem ReplaceParameters="false" TargetFileName="SampleMenu.xaml">SampleMenu.xaml</ProjectItem>
        </Folder>
        <ProjectItem ReplaceParameters="false" TargetFileName="appsettings.json">appsettings.json</ProjectItem>
        <ProjectItem ReplaceParameters="false" TargetFileName="App.xaml">App.xaml</ProjectItem>
        <ProjectItem ReplaceParameters="false" TargetFileName="App.xaml.cs">App.xaml.cs</ProjectItem>
        <ProjectItem ReplaceParameters="false" TargetFileName="MainWindow.xaml">MainWindow.xaml</ProjectItem>
        <ProjectItem ReplaceParameters="false" TargetFileName="MainWindow.xaml.cs">MainWindow.xaml.cs</ProjectItem>
        <ProjectItem ReplaceParameters="false" TargetFileName="MainWindowViewModel.cs">MainWindowViewModel.cs</ProjectItem>
        <ProjectItem ReplaceParameters="false" TargetFileName="RichMenuAction.cs">RichMenuAction.cs</ProjectItem>
        <ProjectItem ReplaceParameters="false" TargetFileName="RichMenuDefsControl.cs">RichMenuDefsControl.cs</ProjectItem>
        <ProjectItem ReplaceParameters="false" TargetFileName="RichMenuProperties.cs">RichMenuProperties.cs</ProjectItem>
        <ProjectItem ReplaceParameters="false" TargetFileName="RichMenuSettings.cs">RichMenuSettings.cs</ProjectItem>
      </Project>
    </TemplateContent>
</VSTemplate>